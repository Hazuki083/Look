<%= form_tag(admin_item_search_path, method: :get) do %>
<%= text_field_tag 'search[content]', '', class: '' %>
<%= select_tag 'search[how]', options_for_select({ "完全一致" => "match", "前方一致" => "forward", "後方一致" => "backward", "部分一致" => "partical"  }) %>
<%= submit_tag '検索', class: 'btn btn-outline-info' %>
<% end %>