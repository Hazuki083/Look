 <div class="contener">
   <div class="row">
     <div class="col-md-2 text-center" style="margin: 30px 0 30px 200px;">
       <h4 style="background-color: #eeeeee;" >カテゴリー編集</h4>
     </div>
   </div>
   <!--urlにはしたいアクションのpathを記述-->
   <%= form_with model:@category, url:admin_category_path(@category), local:true do |f| %>
     <div class="row">
       <div class="col-md-1"></div>
       <div class="col-md-1">カテゴリー</div>
       <div class="col-2"><%= f.text_field :name  %></div>
       <div class="actions">
           <%= f.submit "変更を保存", :class => "btn btn-outline-secondary btn-block" %>
       </div>
     </div>
   <% end %>
</div>